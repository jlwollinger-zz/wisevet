<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity DonoAnimal.
    -->
    <changeSet id="20181122002324-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="animal_id"
                                 baseTableName="dono_animal"
                                 constraintName="fk_dono_animal_animal_id"
                                 referencedColumnNames="id"
                                 referencedTableName="animal"/>

        <addForeignKeyConstraint baseColumnNames="endereco_id"
                                 baseTableName="dono_animal"
                                 constraintName="fk_dono_animal_endereco_id"
                                 referencedColumnNames="id"
                                 referencedTableName="endereco"/>

    </changeSet>
</databaseChangeLog>
